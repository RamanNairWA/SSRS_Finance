  <DataSets>
    <DataSet Name="InvoiceData">
      <Query>
        <DataSourceName>DBSU4PRD</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="?">
            <Value>=Parameters!FromYear.Value &amp; Parameters!FromMonth.Value &amp; 01</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!ToYear.Value &amp; Parameters!ToMonth.Value  &amp; 31</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!FromYear.Value &amp; Parameters!FromMonth.Value &amp; 01</Value>
          </QueryParameter>
          <QueryParameter Name="?">
            <Value>=Parameters!ToYear.Value &amp; Parameters!ToMonth.Value  &amp; 31</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT DISTINCT 'Rental' AS Type, i.CUNO AS CustomerCode, c.CUNM AS CustomerName, c.EXLCNO AS ABN
FROM         LIBT01.EMPINVH0 i, LIBT01.CIPNAME0 c
WHERE     i.CUNO = c.CUNO AND (i.IVDAT8 BETWEEN ? AND ?) AND (i.ASIVNO LIKE 'ES%')
UNION ALL
SELECT DISTINCT 'Parts' AS Type, h.CUNO AS CustomerCode, c1.CUNM AS CustomerName, h.EXLCNO AS ABN
FROM         LIBT01H.MSPHDRS0 h, LIBT01H.MSPSEGS0 s, LIBT01.CIPNAME0 c1, LIBT01H.MSPDETL0 d
WHERE     h.IVNO1 = s.IVNO1 AND h.PAGENO = s.PAGENO AND h.CUNO = c1.CUNO AND s.IVNO1 = d.IVNO1 AND s.WOSGNO = d.WOSGNO AND 
                      (h.IVDAT8 BETWEEN ? AND ?) AND (d.TRXCD LIKE '8%')
ORDER BY CustomerName</CommandText>
      </Query>
      <Fields>
